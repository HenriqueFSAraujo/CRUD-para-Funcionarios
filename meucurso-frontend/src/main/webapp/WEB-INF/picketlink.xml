<PicketLink xmlns="urn:picketlink:identity-federation:config:2.1">
	<PicketLinkSP xmlns="urn:picketlink:identity-federation:config:2.1"
		ServerEnvironment="tomcat" BindingType="POST" RelayState="someURL"
		ErrorPage="/privado/br/gov/prodemge/proddigio/meucurso/visao/servererror.zul" 
		LogOutPage="/privado/br/gov/prodemge/proddigio/meucurso/visao/timeout.zul"> 
		<!-- Substituir as linhas acima pelas linhas abaixo caso queira que o sistema tenha somente parte privada.-->
		<!-- ErrorPage="/privado/br/gov/prodemge/proddigio/meucurso/visao/servererror.zul"  -->
		<!-- LogOutPage="/privado/br/gov/prodemge/proddigio/meucurso/visao/timeout.zul"> -->
		<IdentityURL>${ssc-idp-web.url}
		</IdentityURL>
		<ServiceURL>${meucurso-frontend.url}
		</ServiceURL>
	</PicketLinkSP>
	<Handlers xmlns="urn:picketlink:identity-federation:handler:config:2.1">
		<Handler
			class="br.gov.prodemge.ssc.picketlink.handler.ProdemgeAdap64SamlLogoutHandler" />
		<Handler class="br.gov.prodemge.ssc.picketlink.ProdemgeAdap64SAML2AuthenticationHandler" >
			<Option Key="CLOCK_SKEW_MILIS" Value="10000" />
			<Option Key="REDIRECT_TO_ORIGINAL_URL" Value="false" />
		</Handler>
		<Handler
			class="org.picketlink.identity.federation.web.handlers.saml2.RolesGenerationHandler" />
	</Handlers>
</PicketLink>
